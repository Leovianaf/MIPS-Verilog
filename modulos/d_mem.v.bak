module d_mem (
    input [31:0] address, writeData,
    input memWrite, memRead,
    output reg [31:0] readData
);

    // Declarar a mem처ria interna
    reg [31:0] memory [0:63];

    // L처gica combinacional de escrita
    always @* begin
        if (memWrite) begin
            memory[address] = writeData;
        end
    end

    // L처gica combinacional de leitura
    always @* begin
        if (memRead) begin
            readData = memory[address];
        end else begin
            readData = 32'bz; // Alta imped창ncia
        end
    end

endmodule
